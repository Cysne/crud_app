<div class="container">
  <div class="row justify-content-center">
    <h4 class="mt-4 mb-4 display-4">Detalhes do Post <%= @post.id %></h4>
  </div>
  <div class="row justify-content-center">
    <table class="table">
      <tbody>
        <tr>
          <th>Title:</th>
          <td><%= @post.titulo %></td>
        </tr>
        <tr>
          <th>Description:</th>
          <td><%= @post.Descricao %></td>
        </tr>
      </tbody>
    </table>
    <button type= 'button' class="btn btn-outline-primary"> <%= link_to "Editar", edit_post_path(@post) %></button>
    <button type= 'button' class='btn btn-outline-primary'> <%= link_to "Pagina Principal", root_path %>
  </div>
</div>

